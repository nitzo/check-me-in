<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>EventAttendingUpdaterServlet</servlet-name>
		<servlet-class>com.google.code.checkmein.servlets.EventAttendingUpdaterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EventAttendingUpdaterServlet</servlet-name>
		<url-pattern>/cron/eventattendingupdater</url-pattern>
	</servlet-mapping>	
	<servlet>
		<servlet-name>CheckInServlet</servlet-name>
		<servlet-class>com.google.code.checkmein.servlets.CheckInServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CheckInServlet</servlet-name>
		<url-pattern>/checkin</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>QRCodeGeneratorServlet</servlet-name>
		<servlet-class>com.google.code.checkmein.servlets.QRCodeGeneratorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>QRCodeGeneratorServlet</servlet-name>
		<url-pattern>/qrcodegenerator</url-pattern>
	</servlet-mapping>
		<servlet>
		<servlet-name>DebugDataInitServlet</servlet-name>
		<servlet-class>com.google.code.checkmein.servlets.DebugDataInitServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DebugDataInitServlet</servlet-name>
		<url-pattern>/debugdatainit</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>StatisticsServlet</servlet-name>
		<servlet-class>com.google.code.checkmein.servlets.StatisticsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>StatisticsServlet</servlet-name>
		<url-pattern>/stats</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SettingsServlet</servlet-name>
		<servlet-class>com.google.code.checkmein.servlets.SettingsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SettingsServlet</servlet-name>
		<url-pattern>/settings</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
